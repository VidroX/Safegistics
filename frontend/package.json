{
  "name": "safegistics",
  "version": "1.0.0",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all clean transpile server:dev",
    "start": "npm-run-all clean transpile build server:prod",
    "build": "next build",
    "clear-build": "npm-run-all clean transpile build",
    "next-start": "next start",
    "server:prod": "cross-env NODE_ENV=production node .build/server",
    "server:dev": "cross-env NODE_ENV=development node .build/server",
    "transpile": "babel ./src/server.ts ./src/i18n.ts --extensions \".ts\" --out-dir .build",
    "clean": "rimraf dist-server",
    "analyze": "cross-env ANALYZE=true npm-run-all clear-build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm-run-all clear-build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm-run-all clear-build"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "@babel/cli": "^7.8.4",
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.9.10",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "@material-ui/pickers": "^3.2.10",
    "@nivo/line": "^0.61.1",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@zeit/next-source-maps": "^0.0.3",
    "apollo-boost": "^0.4.7",
    "core-js": "^3.6.5",
    "cross-fetch": "^3.0.4",
    "css-mediaquery": "^0.1.2",
    "date-fns": "^2.0.0-beta.5",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "next": "9.3.4",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^4.2.1",
    "next-transpile-modules": "^3.3.0",
    "next-with-apollo": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.9.0",
    "react-text-mask": "^5.4.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "ua-parser-js": "^0.7.21",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
