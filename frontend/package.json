    "@types/js-cookie": "^2.2.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.34",
    "@types/react-text-mask": "^5.4.6",
    "@types/ua-parser-js": "^0.7.33",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "typescript": "^3.8.3"
  }
